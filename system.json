{
	"id": "projectfu",
	"title": "Fabula Ultima (Project FU)",
	"description": "An Unofficial Fabula Ultima system for FoundryVTT!",
	"version": "#{VERSION}#",
	"compatibility": {
		"minimum": "11",
		"verified": "11"
	},
	"authors": [
		{
			"name": "zarick1342",
			"url": "https://github.com/zarick1342"
		},
		{
			"name": "serpuss"
		},
		{
			"name": "spyrella",
			"url": "https://github.com/spyrella"
		},
		{
			"name": "Shourn",
			"url": "https://github.com/Shourn"
		}
	],
	"scripts": [],
	"esmodules": ["module/projectfu.mjs"],
	"styles": ["styles/css/projectfu.css", "styles/css/rpg-awesome.css"],
	"packs": [
		{
			"label": "Essentials",
			"type": "Item",
			"name": "essentials",
			"path": "packs/essentials",
			"system": "projectfu",
			"ownership": {
				"PLAYER": "OBSERVER",
				"ASSISTANT": "OWNER"
			}
		}
	],
	"languages": [
		{
			"lang": "en",
			"name": "English",
			"path": "lang/en.json"
		},
		{
			"lang": "it",
			"name": "Italiano",
			"path": "lang/it.json"
		},
		{
			"lang": "de",
			"name": "Deutsch",
			"path": "lang/de.json"
		},
		{
			"lang": "fr",
			"name": "Française",
			"path": "lang/fr.json"
		},
		{
			"lang": "es",
			"name": "Español",
			"path": "lang/es.json"
		},
		{
			"lang": "pt-BR",
			"name": "Português (Brasil)",
			"path": "lang/pt_BR.json"
		},
		{
			"lang": "ru",
			"name": "Русский",
			"path": "lang/ru.json"
		}
	],
	"relationships": {
		"requires": [
			{
				"id": "socketlib",
				"type": "module"
			}
		]
	},
	"flags": {
		"hotReload": {
			"extensions": ["css", "hbs", "json"],
			"paths": ["css", "lang", "templates"]
		}
	},
	"socket": true,
	"gridDistance": 5,
	"gridUnits": "ft",
	"primaryTokenAttribute": "resources.hp",
	"secondaryTokenAttribute": "resources.mp",
	"license": "LICENSE.txt",
	"changelog": "CHANGELOG.md",
	"url": "#{URL}#",
	"manifest": "#{MANIFEST}#",
	"download": "#{DOWNLOAD}#"
}
